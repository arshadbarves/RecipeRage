        </VisualElement>
        <ScrollView name="settings-scroll" class="settings-scroll">
            <VisualElement name="settings-container" class="settings-container">
                <VisualElement name="settings-section" class="settings-section">
                    <Label name="section-title" text="AUDIO" class="section-title" />
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="Master Volume" class="setting-label" />
