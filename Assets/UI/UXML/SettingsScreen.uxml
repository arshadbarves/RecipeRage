                        <Label name="setting-value" text="70%" class="setting-value" />
                    </VisualElement>
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="SFX Volume" class="setting-label" />
                        <Slider name="sfx-volume-slider" value="90" high-value="100" class="setting-slider" />
                        <Label name="setting-value" text="90%" class="setting-value" />
                    </VisualElement>
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="Voice Volume" class="setting-label" />
                        <Slider name="voice-volume-slider" value="85" high-value="100" class="setting-slider" />
                        <Label name="setting-value" text="85%" class="setting-value" />
                    </VisualElement>
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="Mute All" class="setting-label" />
                        <Toggle name="mute-toggle" class="setting-toggle" />
                    </VisualElement>
                </VisualElement>
                <VisualElement name="settings-section" class="settings-section">
                    <Label name="section-title" text="GRAPHICS" class="section-title" />
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="Quality" class="setting-label" />
                        <DropdownField name="quality-dropdown" class="setting-dropdown" />
                    </VisualElement>
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="Resolution" class="setting-label" />
                        <DropdownField name="resolution-dropdown" class="setting-dropdown" />
                    </VisualElement>
                    <VisualElement name="setting-row" class="setting-row">
                        <Label name="setting-label" text="Fullscreen" class="setting-label" />
                        <Toggle name="fullscreen-toggle" value="true" class="setting-toggle" />
